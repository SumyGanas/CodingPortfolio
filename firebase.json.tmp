{  "hosting": {
  "site": "alpine-figure-414421",
  "public": "dist",
  "headers": [ {
    "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
    "headers": [ {
      "key": "Access-Control-Allow-Origin",
      "value": "*"
    } ]
  }, {
    "source": "**/*.@(jpg|jpeg|gif|png)",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=7200"
    } ]
  },{
    "source": "**/*.css",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=86400, immutable"
    } ]
  }, {
    "source": "404.html",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=300"
    } ]
  },
  {
    "source": "**",
    "headers": [
    {
      "key": "X-Content-Type-Options",
      "value": "nosniff"
    },
    {
      "key": "X-Frame-Options",
      "value": "DENY"
    },
    {
      "key": "Referrer-Policy",
      "value": "no-referrer"
    } ]
  },
  {
    "source": "/index.html",
  "headers": [
    {
      "key": "Cache-Control",
      "value": "no-cache"
    }
  ]
  }]
  ,
  "cleanUrls": true,
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "build",
    ".env",
    ".env.local",
    ".env.*.local",
    ".DS_Store",
    "npm-debug.log*",
    ".vscode",
    ".firebase",
    "firestore-debug.log",
    "ui-debug.log",
    "dist",
    "apphosting.emulator.yaml",
    ".local.redirects.private.json"
  ],
  "redirects": [],
 "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
},
  "emulators": {
    "singleProjectMode": true,
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    },
    "ui": {
      "enabled": true,
      "port": 4000,
      "host": "0.0.0.0"
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}


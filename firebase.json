{  "hosting": {
  "site": "alpine-figure-414421",
  "public": "dist",
  "cleanUrls": true,
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "build",
    ".env",
    ".env.local",
    ".env.*.local",
    ".DS_Store",
    "npm-debug.log*",
    ".vscode",
    ".firebase",
    "firestore-debug.log",
    "ui-debug.log",
    "dist",
    "apphosting.emulator.yaml"
  ],
  "redirects": [
    {
      "source": "**",
      "destination": "https://www.sumyganas.com",
      "type": 301,
      "condition": {
        "hostname": "sumyganas.com"
      }
    }
  ],
 "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
},
  "emulators": {
    "singleProjectMode": true,
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}


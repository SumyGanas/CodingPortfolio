{  "hosting": {
  "site": "alpine-figure-414421",
  "public": "dist",
  "headers": [ {
    "source": "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
    "headers": [ {
      "key": "Access-Control-Allow-Origin",
      "value": "*"
    } ]
  }, {
    "source": "**/*.@(jpg|jpeg|gif|png|css)",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=7200"
    } ]
  }, {
    "source": "404.html",
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=300"
    } ]
  }, 
  {
    "key": "X-Content-Type-Options",
    "value": "nosniff"
  },
  {
    "key": "X-Frame-Options",
    "value": "DENY"
  },
  {
    "key": "Referrer-Policy",
    "value": "no-referrer"
  }]
  ,
  "cleanUrls": true,
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "build",
    ".env",
    ".env.local",
    ".env.*.local",
    ".DS_Store",
    "npm-debug.log*",
    ".vscode",
    ".firebase",
    "firestore-debug.log",
    "ui-debug.log",
    "dist",
    "apphosting.emulator.yaml"
  ],
  "redirects": [
    {
      "source": "https://sumyganas.com",
      "destination": "https://www.sumyganas.com",
      "type": 301,
      "condition": {
        "hostname": "sumyganas.com"
      }
    }
  ],
 "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
},
  "emulators": {
    "singleProjectMode": true,
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}

